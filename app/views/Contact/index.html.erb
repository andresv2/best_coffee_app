
<center><h3> Subscribe</h3></center>
 <div class="row">
  <div class="col s2">&nbsp;</div>
  <form class="col s8">
    <div class="row">
      <div class="input-field col s6">
        <input id="first_name" type="text" placeholder="First Name" class="validate">
      </div>
      <div class="input-field col s6">
        <input id="last_name" type="text" placeholder="Last Name" class="validate">

      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <input id="Neighbourhood" type="text" placeholder="Neighbourhood" class="validate">
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <input id="email" type="email" class="validate">
        <label for="email">Email</label>
      </div>
    </div>
    <div class="row">
      <a href="#" class="submitButton">Submit</a>
    </div>
  </form>
  <div class="col s2">&nbsp;</div>
</div>
